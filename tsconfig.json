{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2016",
    "moduleResolution": "node",
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "alwaysStrict": true,
    "sourceMap": false,
    "outDir": "./dist"
  },
  "include": [
    "./index.js",
    "./**.ts",
    "./src/**.ts"
  ],
  "lib": [
    "es2016"
  ]
}

