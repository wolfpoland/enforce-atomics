{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2016",
    "moduleResolution": "node",
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "alwaysStrict": true,
    "sourceMap": false,
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "lib": ["es2016"]
}
